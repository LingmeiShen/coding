def factorial(n):
    if n<2:
        return 1
    return n*factorial(n-1)
print factorial(6)
720
In [3]:
def gcd(a, b):
# write this function...

    c = max(a,b) % min(a,b)
    d = min(a,b) % c
    while d>0:
        c=d
        d= min(a,b)%c
    return c
    
print gcd(144, 1024)
16
